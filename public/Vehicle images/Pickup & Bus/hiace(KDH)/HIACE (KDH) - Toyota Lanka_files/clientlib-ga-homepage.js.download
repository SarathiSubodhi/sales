(function(CT){try{document.querySelectorAll("section.cpl-home-content .cpl-sec-cont a").forEach(function(section,i){section.addEventListener("click",function(e){var data={eventCategory:CT.CATEGORY_C,eventAction:CT.ACTION_LINK_CLICK};data.eventLabel="HomepageTile:"+CT.cleanDomainAndLang(this.getAttribute("href"));CT.push(data);});});document.querySelectorAll("section#cpl-homepage-models .cpl-cta-wrapper a").forEach(function(section,i){section.addEventListener("click",function(e){var data={eventCategory:CT.CATEGORY_C,eventAction:CT.ACTION_LINK_CLICK};data.eventLabel=CT.ACTION_MS+":"+CT.cleanDomainAndLang(this.getAttribute("href"));CT.push(data);});});}catch(error){console.warn(error);}})(Convertium.Tagging);